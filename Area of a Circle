#include <iostream>
#include <cmath>
#include <iomanip>
using namespace std;

int main()
{
    const long double PI = 3.141592653;
    long double R;
    cin>> R;
    cout << setprecision(9) << PI*pow(R,2)<< endl;
    return 0;
}
