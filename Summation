Link: http://codeforces.com/group/MWSDmqGsZm/contest/219774/problem/A

#include <iostream>
#include <cmath>
using namespace std;

int main()
{
    int n;
    long long sum=0;
    cin>>n;

    long long int A[n];

    for (int i=0; i<n; i++)
    {
        cin>> A[i];
        sum += A[i];
    }
    cout<<abs(sum);
    return 0;
}
