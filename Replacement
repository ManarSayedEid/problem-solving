Link: http://codeforces.com/group/MWSDmqGsZm/contest/219774/problem/C

#include <iostream>
using namespace std;

int main()
{
  int n;
  cin>>n;

  int A[n];

for(int i=0; i<n; i++)
    cin>>A[i];

for(int i=0; i<n; i++){
    if (A[i]==0)
        cout<<0<<" ";
    else if (A[i]>0)
        cout<<1<<" ";
    else
        cout<<2<<" ";
}

    
    return 0;
}
