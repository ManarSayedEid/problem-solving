Link: https://codeforces.com/group/MWSDmqGsZm/contest/219158/problem/R

#include <iostream>
using namespace std;

int main()
{
  int N;
  cin>>N;

  int years = N/365;
  int remain = N%365;

  int months = remain/30;

  int days = remain%30;

  cout<<years<< " years\n"
      <<months<< " months\n"
      <<days<< " days\n";
  
  
    
    return 0;
}
