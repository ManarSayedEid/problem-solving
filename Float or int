Link: https://codeforces.com/group/MWSDmqGsZm/contest/219158/problem/U

#include <iostream>
using namespace std;

int main()
{
    double x;
    cin>>x;
    
    if (x - int(x) == 0)
       cout<<"int "<<int(x);
    else{
       cout<<"float "<<int(x)<< " "
           << x-int(x)<<endl;
    }
    
    return 0;
}
