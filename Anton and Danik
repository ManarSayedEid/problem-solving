Link: http://codeforces.com/contest/734/problem/A

#include <iostream>

using namespace std;

int main()
{
    int games,anton=0,danic=0;
    cin>>games;
    string str;
    cin>>str;

for (int i=0; i<games; i++){
    if (str[i] == 'A')
        anton++;
    else
        danic++;
}

if (anton > danic)
    cout<<"Anton";
else if (anton < danic)
    cout<<"Danik";
else
    cout<<"Friendship";
    
return 0;
}
