Link: http://codeforces.com/contest/9/problem/A

#include <iostream>

using namespace std;

int main()
{
    int y,w;
cin>>y>>w;

int mx = max(y,w);

if (mx==1)
    cout<<1<<"/"<<1;
else if (mx==2)
    cout<<5<<"/"<<6;
else if (mx==3)
    cout<<2<<"/"<<3;
else if (mx==4)
    cout<<1<<"/"<<2;
else if (mx==5)
    cout<<1<<"/"<<3;
else
    cout<<1<<"/"<<6;
    
    
      
    
  return 0;
}
