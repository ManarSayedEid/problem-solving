#include <iostream>
using namespace std;

int main()
{
    while(true)
    {
    int num;
    cin>> num;
    
    if (num == 1999)
    {
       cout<<"Correct"<<endl;
       break;
    }
    else
       cout<<"Wrong"<<endl;
  
    }
    return 0;
}
