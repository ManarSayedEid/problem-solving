Link: http://codeforces.com/group/MWSDmqGsZm/contest/219432/problem/G

#include <iostream>
using namespace std;

int main()
{
      int t;
    cin>>t;

    while(t>0)
    {
        long long int fact=1;
        int num;
        cin>>num;

        
        for(int i =2; i<=num ; i++)
           fact *= i;


        cout<<fact<<endl;
        t--;
    }
     
    return 0;
}
