Link: https://codeforces.com/group/MWSDmqGsZm/contest/219158/problem/V

#include <iostream>
using namespace std;

int main()
{
    int a,b;
    char x;
    cin>> a>>x>>b;
    
    if (x == '='){
        if (a-b == 0)
           cout<<"Right";
        else
           cout<<"Wrong";
    }
    else if (x == '>'){
        if (a-b > 0)
           cout<<"Right";
        else 
           cout<<"Wrong";
    }
    else if (x == '<'){
        if (a-b < 0)
           cout<<"Right";
        else 
           cout<<"Wrong";
    }
    
    return 0;
}
