Link: http://codeforces.com/contest/136/problem/A

#include <iostream>

using namespace std;

int main()
{
    int n;
    cin>>n;
    int num, freq[n];
    
    for (int i=1; i<=n; i++){
        cin>>num;
        freq[num]=i;
    }
    
    for (int i=1; i<=n; i++)
       cout<<freq[i]<<" ";
    
      
    
  return 0;
}
