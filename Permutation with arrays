Link: https://codeforces.com/group/MWSDmqGsZm/contest/219774/problem/R

#include <iostream>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int arr_a[n], arr_b[n];
    int sum_a=0 , sum_b=0;


    for (int i=0; i<n; i++){
        cin>>arr_a[i];
        sum_a += (arr_a[i] % 10);
    }
        
        
    for (int i=0; i<n; i++){
        cin>>arr_b[i];    
        sum_b += (arr_b[i] % 10);
    }

    if (sum_a == sum_b)
       cout<<"yes";
    else
       cout<<"no";

    return 0;
}
