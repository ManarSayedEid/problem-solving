Link: https://codeforces.com/group/MWSDmqGsZm/contest/219158/problem/O

#include <iostream>
using namespace std;

int main()
{
    int a,b;
    char x;
    cin>> a>>x>>b;
    
    if (x == '+')
        cout<< a+b ;
    if (x == '-')
        cout<< a-b ; 
    if (x == '*')
        cout<< a*b ;
    if (x == '/')
        cout<< a/b ;    
       
    return 0;
}
