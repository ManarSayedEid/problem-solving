Link: https://codeforces.com/group/MWSDmqGsZm/contest/219774/problem/V

#include <iostream>

using namespace std;

int main()
{
    int n,m;
    cin>>n>>m;
    int arr[n], freq[100001]={0};


    for (int i=0; i<n; i++){
        cin>>arr[i];
        freq[arr[i]]++;
    }

    for (int i=1; i<=m; i++){
          cout<<freq[i]<<endl;
    }
    return 0;
}
