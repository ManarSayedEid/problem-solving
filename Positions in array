Link: http://codeforces.com/group/MWSDmqGsZm/contest/219774/problem/D

#include <iostream>
using namespace std;

int main()
{
  int n;
  cin>>n;


double A[n];

for(int i=0; i<n; i++)
    cin>>A[i];


for (int i=0; i<n ; i++)
{
    if (A[i]<=10)
        cout<<"A["<<i<<"] = "<<A[i]<<endl;
}
    
    return 0;
}
