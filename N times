Link: https://codeforces.com/group/MWSDmqGsZm/contest/223205/problem/H

#include <iostream>
using namespace std;


void N_times(int t){
    int num;
    char ch;
    while(t--){
        cin>>num>>ch;
        
    for (int i=1; i< num; i++)
       cout<<ch<<" ";
       
    cout<<ch<<endl;
    }
}

void test(){
    int t;
    cin>>t;
    N_times(t);
}

int main()
{
    test();

    return 0;
}
