Link: http://codeforces.com/group/MWSDmqGsZm/contest/219432/problem/H

#include <iostream>
using namespace std;

int main()
{
  int num;
  cin>>num;
  
  string response = "YES";
  
     for (int i = 2; i<num  ; i++)
     {
        if (num%i == 0 )
           response = "NO";
     }
     
     cout<<response;
    
    return 0;
}
