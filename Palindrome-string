Link: https://codeforces.com/group/MWSDmqGsZm/contest/219856/problem/I

#include <iostream>
using namespace std;

int main()
{
    string str;
    cin>>str;
    bool is_palindrome=true;

    for (int i=0; i<str.size()/2; i++){
        if (str[i] != str[str.size()-i-1]){
            is_palindrome=false;
            break;
        }
    }

    if (is_palindrome)
        cout<<"YES";
    else
        cout<<"NO";

        
        
    return 0;
}
