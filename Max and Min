Link: https://codeforces.com/group/MWSDmqGsZm/contest/219158/problem/K

#include <iostream>
using namespace std;

int main()
{
    int a,b,c,min,max;
    cin>> a>>b>>c;
    
    min = a;
    max = b;
    
    if (min > b)
       min = b;

    if (min>c)
       min = c;
    
    if (a> max)
       max = a;

    if (c>max)
       max = c;
       
    cout<< min << " " << max ;

    return 0;
}
